                <script>
                $(document).ready(function(){

                $("#vegCarousel").carousel();


                $(".item1").click(function(){
                $("#vegCarousel").carousel(0);
                });
                $(".item2").click(function(){
                $("#vegCarousel").carousel(1);
                });
                $(".item3").click(function(){
                $("#vegCarousel").carousel(2);
                });
                $(".item4").click(function(){
                $("#vegCarousel").carousel(3);
                });


                $(".left").click(function(){
                $("#vegCarousel").carousel("prev");
                });
                $(".right").click(function(){
                $("#vegCarousel").carousel("next");
                });
                });
                </script>
